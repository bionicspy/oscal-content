    "components": [
      {
        "uuid": "e1a0a2c7-3b34-4a90-8d60-633f93a6a0a0",
        "type": "service",
        "title": "Azure Front Door (Standard/Premium)",
        "description": "Azure Front Door is Microsoftâ€™s modern CDN and global application entry point providing anycast-based edge termination, advanced routing, optional caching, WAF at the edge, end-to-end TLS, and optional private origin connectivity (Premium).",
        "props": [
          { "name": "afd:subscription-id", "value": "<azure-subscription-guid>" },
          { "name": "afd:resource-group", "value": "<resource-group-name>" },
          { "name": "afd:profile-name", "value": "<afd-profile-name>" },
          { "name": "afd:sku", "value": "Premium" },
          { "name": "afd:waf:policy-resource-id", "value": "<resourceId-of-waf-policy-or-empty>" },
          { "name": "afd:waf:mode", "value": "Prevention" },
          { "name": "afd:tls:policy", "value": "<predefined-or-custom-policy-name>" },
          { "name": "afd:tls:min-version", "value": "TLS1.2" },
          { "name": "afd:tls:client-tls13", "value": "enabled" },
          { "name": "afd:origin:private-link", "value": "enabled" },
          { "name": "afd:origin:fdid-header-enforced", "value": "true" },
          { "name": "afd:diagnostics:logs", "value": "Access,HealthProbe,WAF" },
          { "name": "afd:diagnostics:destination", "value": "LogAnalytics;Storage;EventHub" }
        ],
        "links": [
          {
            "href": "https://learn.microsoft.com/en-us/azure/frontdoor/end-to-end-tls",
            "rel": "reference",
            "text": "TLS encryption with Azure Front Door"
          },
          {
